package exam2_tests;

import java.util.ArrayList;

public class problems {
	
	public static String blankVowels(String str) {
		ArrayList<Character> vowels = new ArrayList<Character>();
		vowels.add('a');
		vowels.add('e');
		vowels.add('i');
		vowels.add('o');
		vowels.add('u');
		
		char[] word = str.toCharArray();
		
		for(int i = 0; i < word.length; i++) {
			if(vowels.contains(word[i])) {
				word[i] = '_';
			}
		}
			
		return new String(word);
	}
	
	public static int[] removeDuplicates(int[] dupsArr) {
		ArrayList<Integer> noDupsList = new ArrayList<Integer>();
		
		for(int i = 0; i < dupsArr.length; i++) {
			if(!noDupsList.contains(dupsArr[i])) {
				noDupsList.add(dupsArr[i]);
			}
		}
		
	}
	
	public static void main(String args[]) {
//		System.out.println(blankVowels("volume"));
	}
	
}
